<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Module/etc/module.xsd">
    <!-- Specify the module name and its setup version -->
    <module name="Maginium_AdminCleanMenu" setup_version="1.0.0">
        <!-- Define module dependencies -->
        <sequence>
            <!-- Ensure Magento_Backend module is loaded first -->
            <!-- Specify the module name and its setup version -->
            <module name="Magento_Backend" />
            <!-- Ensure Magento_Config module is loaded before this module -->
            <!-- Specify the module name and its setup version -->
            <module name="Magento_Config" />
        </sequence>
    </module>
</config>
